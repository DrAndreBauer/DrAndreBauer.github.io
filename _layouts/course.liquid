---
layout: default
---
        <div class="post">

          <header class="post-header">
            <h1 class="post-title">{{ page.name }}</h1>
          </header>

          <article>
              <div class="info mt-4">
                <div class="info-row">
                  <div class="info-label"><strong>Course number:</strong></div>
                  <div class="info-value">{{ page.title }}</div>
                </div>
                <div class="info-row">
                  <div class="info-label"><strong>Semester:</strong></div>
                  <div class="info-value">{{ page.semester }}</div>
                </div>
                <div class="info-row">
                  <div class="info-label"><strong>Day:</strong></div>
                  <div class="info-value">{{ page.day }}</div>
                </div>
                <div class="info-row">
                  <div class="info-label"><strong>Location:</strong></div>
                  <div class="info-value">{{ page.location }}</div>
                </div>
              </div>

              <p></p>
              {{ content }}
          </article>

        </div>